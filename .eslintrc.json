{
  "extends": ["wk/loose", "wkvue/loose"],
  "plugins": ["eslint-plugin-local-rules"],
  "globals": {},
  "rules": {
    "local-rules/no-cross-import": "error",
    "vue/no-mutating-props": "warn",
    "no-irregular-whitespace": "off"
  },
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "es2017": true
  }
}
